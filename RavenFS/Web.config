  </system.web>
  <system.webServer>
    <handlers>
      <add name="Route Requests" path="*" verb="*" type="RavenFS.Infrastructure.RouterHandlerFactory, RavenFS"/>
    </handlers>
  </system.webServer>
</configuration>
